{
  "name": "snow-inn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Nikita Romanov",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "serve": "http-server ./build --port 3000 -o",
    "start:json:server": "node ./json-server/index.js",
    "lint": "eslint src config --ext .ts,.tsx,.js",
    "lint:fix": "eslint src config --ext .ts,.tsx,.js --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "storybook": "storybook dev -p 6006 -c config/storybook",
    "build:storybook": "storybook build -c config/storybook",
    "test:ui": "dotenv -e .env.chromatic -- npx chromatic",
    "test:ui:changed": "dotenv -e .env.chromatic -- npx chromatic --only-changed"
  },
  "repository": {
    "type": "git",
    "url": "git+https:"
  },
  "bugs": {
